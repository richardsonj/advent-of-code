{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-use-before-define": "off",
    "no-restricted-syntax": "off",
    "no-plusplus": "off",
    "radix": "off",
    "no-continue": "off",
    "no-param-reassign": "off",
    "default-case": "off",
    "no-console": "off",
    "consistent-return": "warn",
    "no-unused-vars": "warn",
    "camelcase": "off"
  }
}
